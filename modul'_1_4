print("Введи любую строку:")
my_string=input()
print(type(my_string))
print("Кол-во символов в стоке:")
print(len(my_string))
print("Ваша строка в Верхнем регистре:")
print(my_string.upper())
print("Ваша строка в нижнем регистре:")
print(my_string.lower())
print("Ваша строка без пробелов:")
print(my_string.replace(' ',''))
print("Первый символ строки:")
print(my_string[0])
print("Последний символ строки:")
print(my_string[-1])
