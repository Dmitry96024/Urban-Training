immutable_var= 1, "apple", True
print(immutable_var)
# immutable_var[0] = 100 Вывод: TypeError: 'tuple' object does not support item assignment
# но если сделать список то будет так:
immutable_var_1=[(1,2,3), "apple", True]
print(immutable_var_1)
immutable_var_1[2]=False
print(immutable_var_1)
mutable_list=[1, "Dmitriy", False]
print(mutable_list)
mutable_list.remove(False)
print(mutable_list)
mutable_list.append("Alex")
print(mutable_list)
mutable_list.extend("Yuliya")
mutable_list.extend(["Yuliya", "25"])
print(mutable_list)
print(immutable_var_1+mutable_list)
